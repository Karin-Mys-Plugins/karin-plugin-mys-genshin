{
  "name": "karin-plugin-mys-genshin",
  "version": "1.0.3",
  "author": "babanbang",
  "type": "module",
  "description": "karin-plugin-mys-genshin",
  "homepage": "https://github.com/Karin-Mys-Plugins/karin-plugin-mys-genshin",
  "bugs": {
    "url": "https://github.com/Karin-Mys-Plugins/karin-plugin-mys-genshin/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Karin-Mys-Plugins/karin-plugin-mys-genshin.git"
  },
  "scripts": {
    "app": "node lib/app.js",
    "build": "npm run type-check && npm run build:css && tsdown",
    "build:css": "tailwindcss -i src/template/tailwind.css -o resources/styles/karin-plugin-mys-genshin.css",
    "type-check": "tsc --noEmit",
    "dev": "cross-env EBV_FILE=\"development.env\" node --import tsx src/app.ts",
    "dev:css": "tailwindcss -i src/template/tailwind.css -o resources/styles/karin-plugin-mys-genshin.css --watch",
    "pub": "npm publish --access public"
  },
  "exports": {
    "./meta/avatars": {
      "types": "./lib/exports/meta/avatars/index.d.ts",
      "development": "./src/exports/meta/avatars/index.ts",
      "default": "./lib/exports/meta/avatars/index.js"
    }
  },
  "main": "lib/index.js",
  "engines": {
    "karin-plugin-mys-core": "^1.0.25",
    "karin": "^1.12.0"
  },
  "devDependencies": {
    "@karinjs/plugin-puppeteer": "^1.0.21",
    "@types/express": "^5.0.1",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20.17.8",
    "@types/react": "^19.2.7",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.2",
    "karin-plugin-mys-core": "https://pkg.pr.new/karin-plugin-mys-core@dd5c92c",
    "neostandard": "^0.11.9",
    "node-karin": "^1.13.0",
    "tailwindcss": "^3.4.17",
    "tsdown": "^0.18.2",
    "tsx": "^4.19.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "karin-plugin-mys-core": "^1.0.25"
  },
  "karin": {
    "main": "src/index.ts",
    "apps": [
      "lib/apps"
    ],
    "ts-apps": [
      "src/apps"
    ],
    "web": "./lib/web.config.js",
    "ts-web": "./src/web.config.ts",
    "static": [
      "resources"
    ],
    "files": [
      "config",
      "data",
      "resources"
    ]
  },
  "files": [
    "LICENSE",
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "resources",
    "!lib/app.js",
    "!lib/app.d.ts",
    "!lib/index.d.ts",
    "!lib/web.config.d.ts",
    "!lib/apps/**/*.d.ts"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
